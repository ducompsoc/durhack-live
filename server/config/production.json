{
  "csrf": {
    "cookieName": "__Host-psifi.x-csrf-token",
    "cookieOptions": {
      "secure": true
    }
  },
  "session": {
    "cookie": { "secure": true }
  },
  "jsonwebtoken": {
    "algorithm": "rsa",
    "accessTokenPublicKeyFilePath": "keys/accessToken.pub.pem",
    "accessTokenPrivateKeyFilePath": "keys/accessToken.pem",
    "refreshTokenPublicKeyFilePath": "keys/refreshToken.pub.pem",
    "refreshTokenPrivateKeyFilePath": "keys/refreshToken.pem"
  }
}