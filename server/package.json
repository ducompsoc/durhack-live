{
  "name": "durhack-live",
  "version": "1.0.0",
  "private": true,
  "description": "Server for DurHack event site",
  "type": "module",
  "scripts": {
    "start": "npx cross-env NODE_ENV=production node --import extensionless/register ./dist/main.js",
    "dev": "tsx ./src/main",
    "dev:watch": "tsx watch ./src/main",
    "build": "npm run format:write && npm run compile",
    "ci-test": "jest --passWithNoTests",
    "format:write": "prettier --write './**/*.ts'",
    "format:check": "prettier --check './**/*.ts'",
    "lint": "npx eslint . -c ./.eslintrc.yml",
    "compile": "npx tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "test": "jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run package"
  },
  "author": "Ethan Waite <ethan.w.waite@durham.ac.uk>",
  "contributors": [
    "Joe Clack <durhack@joeclack.me.uk>",
    "Jacob Wrenn <jacob@wrenn.me>"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/express-mysql-session": "^3.0.0",
    "@types/express-session": "^1.17.7",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/method-override": "^0.0.32",
    "@types/node": "^20.5.1",
    "@types/passport": "^1.0.12",
    "@types/passport-http-bearer": "^1.0.37",
    "@types/passport-local": "^1.0.35",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "exitzero": "^1.0.1",
    "extensionless": "^1.9.6",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.2",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@node-oauth/express-oauth-server": "github:node-oauth/express-oauth-server#c58132f",
    "@node-oauth/oauth2-server": "^4.3.0",
    "body-parser": "github:expressjs/body-parser#2.x",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "csrf-csrf": "^2.3.0",
    "email-validator": "^2.0.4",
    "express": "github:expressjs/express#5.0",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "http-errors": "^2.0.0",
    "jose": "^4.14.4",
    "lodash": "^4.17.21",
    "mailgun.js": "^9.2.0",
    "method-override": "^3.0.0",
    "mysql2": "^3.6.0",
    "node-fetch": "^3.3.2",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "socket.io": "^4.7.2",
    "tsc-alias": "^1.8.8",
    "zod": "^3.22.4"
  }
}
